<div class="cart-template">
  <h1>Your Cart</h1>
  <hr>
  <BR />
  <table>
    <tr>
      <th>Item Name</th>
      <th>Item Qty</th>
      <th>Item price</th>
    </tr>
    {{#each items as |item| }} 
    {{log item}}
      <tr>
        <td>{{item.item_id}}</td>
        <td>{{item.quantity}}</td>
        <td>{{item.price}}</td>
      </tr>
  </table>
    {{/each}}
  <div>
    <h3>Total: </h3>
    {{!-- <button class="btn btn-warning" role="submit" id="checkout-btn">Checkout</button> --}}
  </div>
</div>