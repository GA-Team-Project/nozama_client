<div class="row">
{{#each items as |item|}}
  {{log item}}

  <div class="col-sm-6 col-md-3 item-image">
    <div class="thumbnail">
      <img src={{item.url}} alt="lego figure">
      <div class="caption">
        <h3>{{item.name}}</h3>
        <p>
          <ul data-id="{{item.id}}">
            <b>Price:</b> ${{item.price}}</>
            <li><b>Description:</b> {{item.description}}</li>
            <li><b>Weight:</b> {{item.weight}}g</li>
            <li><b>Dimensions:</b> {{item.dimensions}}</li>
          </ul>
        </p>
        <p><a href="#" class="btn btn-warning" role="button">Add to cart</a></p>
      </div>
    </div>
  </div>
{{/each}}
</div>
