{{#each items as |item|}}
  {{log item}}


 <!-- Developers, why are we using data-id and not just id? -->
  <h1 data-id="{{item.id}}" class="my-item"> <small>{{item.name}}</small></h1>
  <h4 data-id="{{item.id}}">
      {{item.price}}
      {{item.description}}
      {{item.weight}}
      {{item.dimensions}}
  </h4>
  <p><button type="submit">Remove</button></p>


{{/each}}
